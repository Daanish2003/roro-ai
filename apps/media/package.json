{
  "name": "media",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "proto:generate": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=src/proto --js_out=import_style=esm,binary:src/proto --grpc_out=grpc_js:src/proto -I src/proto src/proto/*.proto"
  },
  "dependencies": {
    "@deepgram/sdk": "^3.9.0",
    "@google/generative-ai": "^0.21.0",
    "@grpc/grpc-js": "^1.12.6",
    "@grpc/proto-loader": "^0.7.13",
    "@langchain/core": "^0.3.39",
    "@langchain/google-genai": "^0.1.8",
    "@langchain/groq": "^0.1.3",
    "@langchain/langgraph": "^0.2.49",
    "@langchain/mongodb": "^0.1.0",
    "@roro-ai/database": "workspace:*",
    "@socket.io/redis-adapter": "^8.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fluent-ffmpeg": "^2.1.3",
    "grpc": "^1.24.11",
    "jose": "^5.9.6",
    "langchain": "^0.3.15",
    "mediasoup": "^3.15.3",
    "mongodb": "^6.13.1",
    "node-vad": "^1.1.4",
    "onnxruntime-node": "^1.20.1",
    "opus-decoder": "^0.7.7",
    "opusscript": "^0.1.1",
    "prism-media": "^1.3.5",
    "queue-typescript": "^1.0.1",
    "redis": "^4.7.0",
    "rtp-parser": "^1.0.0",
    "rtp.js": "^0.13.1",
    "sherpa-onnx-node": "^1.10.45",
    "socket.io": "^4.8.1",
    "sox-stream": "^2.0.6",
    "ts-proto": "^2.6.1",
    "uuid": "^11.0.5",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@roro-ai/eslint-config": "workspace:*",
    "@roro-ai/typescript-config": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^22.12.0",
    "@types/rtp-parser": "^1.0.4",
    "@types/ws": "^8.5.14",
    "grpc-tools": "^1.13.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "node-loader": "^2.1.0",
    "onnxruntime-common": "^1.20.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3"
  }
}
