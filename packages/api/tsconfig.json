{
    "compilerOptions": {
      "target": "ES2020",
      "module": "NodeNext",
      "moduleResolution": "nodenext",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }
  